{
  "name": "Roger Watcher",
  "short_name": "rogerwatcher",
  "description": "A validator for Universal Analytics implementations",
  "version": "1.1.0",
  "icons": {
    "16": "icons/min16.png",
    "36": "icons/icon36.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "devtools_page": "devtools.html",
  "minimum_chrome_version": "49.0",
  "web_accessible_resources": [
    "*"
  ],
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; img-src *; object-src 'self'",
  "manifest_version": 2
}